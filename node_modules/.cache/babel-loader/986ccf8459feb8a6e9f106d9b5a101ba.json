tore = createStore({\n  modules: {\n    nubmers: counterModules\n  },\n  state() {\n    return {\n      isLoggedIn: false\n    };\n  },\n  mutations: {\n    setAuth(state, payload) {\n      state.isLoggedIn = payload.isAuth;\n    }\n  },\n  actions: {\n    login(context) {\n      context.commit('setAuth', { isAuth: true });\n    },\n    logout(context) {\n      context.commit('setAuth', { isAuth: false });\n    }\n  },\n  getters: {\n    userIsAuthen(state) {\n      return state.isLoggedIn;\n    }\n  }\n});\nexport default store;\n"]}]}